<% if @postattachments %>
  <div class="row">
    <% @postattachments.each do |attachment| %>
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading preview">
          <%= image_tag attachment.picture.url() %>
        </div>
        <div class="panel-body">
          <span class="pull-right">
          </span>
        </div>
      </div>
    </div>
    <% end %>
  </div>
<% end %>